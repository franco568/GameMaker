            <string>//Draw UI
draw_sprite(sprite_index, image_index, x, y);

//Keep default color
color = draw_get_colour();

//Change font and color
draw_set_font(font_gameover);
draw_set_colour(c_white);

//Concat in string the player coins
scoreText = string(obj_viking.coins);

//Adjust text postion
posText = x + 140;
posText -= (string_width(scoreText));

//Draw Scoore
draw_text(posText,y-65, scoreText);

//Get score previous saved
var sc = real(src_get_value('score'));

//If actual coins more than saved coins(score), draw a New highScore message
if obj_viking.coins &gt; 0 and obj_viking.coins == sc {
    strScore =  'New Highscore!';
    draw_set_font(font_highscore);
    draw_text(x - string_width(strScore)/2,y + 30, strScore);
}


//Ge manually button position square
x1 = x - 140;
y1 = y + 150
x2 = x + 150;
y2 = y + 200;

//Check is end game ui is visivle and mouse was clicked inside button to restart
if visible and mouse_check_button_released(mb_left){
    if mouse_x &gt; x1 and mouse_x &lt; x2
    and mouse_y &gt; y1 and mouse_y &lt; y2{
        room_restart();
    }
}

//draw default color
draw_set_colour(color);
</string>
